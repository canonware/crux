print "Test begin"

for (matrixStrs, charType) in ((dna, Crux.Character.Dna),
  (protein, Crux.Character.Protein)):
    print "=== charType: %r ===" % charType
    for s in matrixStrs:
        matrix = Crux.CTMatrix.CTMatrix(s, charType)
        matrix.fastaPrint(outFile=sys.stdout)
        a = Crux.CTMatrix.Alignment(matrix=matrix)

        for scoreGaps in (True, False):
            print "=== logdetDists(scoreGaps=%r) ===" % (scoreGaps)
            try:
                for allowNan in (True, False):
                    m = a.logdetDists(scoreGaps=scoreGaps, allowNan=allowNan)
                    m.render(distFormat="%.6e", outFile=sys.stdout)
            except:
                import sys

                error = sys.exc_info()
                print "Exception %s: %s" % (error[0], error[1])

print "Test end"
